 
CREATE TABLE Products (
    ProductID INT AUTO_INCREMENT PRIMARY KEY,
    ProductName VARCHAR(100) NOT NULL,
    Description TEXT,
    Price DECIMAL(10, 2) NOT NULL,
    Stock INT NOT NULL,
    Category VARCHAR(50),
    CreatedAt TIMESTAMP DEFAULT CURRENT_TIMESTAMP
);

-- Insert values into Products table
INSERT INTO Products (ProductName, Description, Price, Stock, Category) VALUES
('Laptop', 'High-performance laptop', 10000.99, 10, 'Electronics'),
('Smartphone', 'Latest model smartphone', 9000.99, 20, 'Electronics'),
('Book', 'Fictional novel', 19.99, 50, 'Books');
